<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="transmutableView">
        <attr name="text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DiscreteSlider">
        <attr name="dotCount" format="integer" />
        <attr name="firstColor" format="color" />
        <attr name="barColor" format="color" />
    </declare-styleable>

    <declare-styleable name="IndicatedCircle">
        <attr name="ringColor" format="color" />
        <attr name="ringUnselectedColor" format="color" />
        <attr name="ringWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IndicatedProgressView">
        <attr name="progressRingWidth" format="dimension" />
    </declare-styleable>
</resources>