<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="transmutableView">
        <attr name="text" format="string"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DiscreteSlider">
        <attr name="rangeCount" format="integer"/>
        <attr name="filledColor" format="color"/>
        <attr name="emptyColor" format="color"/>
        <attr name="barHeightPercent" format="float"/>
        <attr name="slotRadiusPercent" format="float"/>
        <attr name="sliderRadiusPercent" format="float"/>
    </declare-styleable>

    <declare-styleable name="CombinedSlider">
        <attr name="combined_icon" format="reference" />
        <attr name="combined_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="IndicatedCircle">
        <attr name="ringColor" format="color" />
        <attr name="ringUnselectedColor" format="color" />
        <attr name="ringWidth" format="dimension" />
    </declare-styleable>

</resources>