<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="transmutableView">
        <attr name="text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DiscreteSlider">
        <attr name="dotCount" format="integer" />
        <attr name="firstColor" format="color" />
        <attr name="barColor" format="color" />
    </declare-styleable>

    <declare-styleable name="IndicatedCircle">
        <attr name="ringColor" format="color" />
        <attr name="ringUnselectedColor" format="color" />
        <attr name="ringWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CheckProgressView">
        <attr name="progressRingWidth" format="dimension" />
        <attr name="progressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MicrophoneVolume">
        <attr name="round_color" format="color" />
        <attr name="minDiameter" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="font" format="string" />
    </declare-styleable>

    <declare-styleable name="DeletableEditText">
        <attr name="right_drawable" format="reference" />
    </declare-styleable>

</resources>